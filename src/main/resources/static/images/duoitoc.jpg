                                                    $errorHTML += '</ul>';
                                                $errorHTML += '</div>';

                                            $form.find('.tdb-s-fc-inner').append($errorHTML);
                                        }

                                        // Remove the loading state for the form
                                        $form.removeClass('tdb-s-content-loading');
                                    }
                                });
                            }

                        });

